<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <exclude
            domain="sharedpref"
            path="evernote_jobs.xml" />
        <exclude
            domain="database"
            path="evernote_jobs.db" />
        <exclude
            domain="file"
            path="." />
        <!-- NMC: MoEngage backup rules -->
        <exclude
            domain="database"
            path="MOEInteractions" />
        <exclude
            domain="sharedpref"
            path="pref_moe.xml" />
        <!-- NMC: end -->
    </cloud-backup>
    <device-transfer>
        <!-- NMC: MoEngage backup rules -->
        <exclude
            domain="database"
            path="MOEInteractions" />
        <exclude
            domain="sharedpref"
            path="pref_moe.xml" />
        <!-- NMC: end -->
    </device-transfer>
</data-extraction-rules>
